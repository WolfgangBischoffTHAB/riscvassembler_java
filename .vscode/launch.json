    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "App",
            "request": "launch",
            "mainClass": "com.mycompany.app.App",
            "projectName": "riscv"
        },
        {
            "type": "java",
            "name": "Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "App",
            "request": "launch",
            "mainClass": "com.mycompany.app.App",
            "projectName": "my-app"
        },
        {
            "type": "venus",
            "request": "launch",
            "name": "Launch current file",
            "program": "${file}",
            "stopOnEntry": true,
            "stopAtBreakpoints": true,
            "openViews": [],
            "ledMatrixSize": {
                "x": 10,
                "y": 10
            }
        },
        {
            "type": "venus",
            "request": "launch",
            "name": "Launch current file with all options",
            "program": "${file}",
            "stopOnEntry": true,
            "stopAtBreakpoints": true,
            "openViews": [
                "Robot",
                "LED Matrix",
                "Seven Segment Board"
            ],
            "ledMatrixSize": {
                "x": 10,
                "y": 10
            }
        }
    ]
}